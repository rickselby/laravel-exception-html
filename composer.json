{
  "name": "rickselby/laravel-exception-html",
  "description": "Generate HTML output for an exception",
  "keywords": ["laravel", "exception", "html"],
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Rick Selby",
      "email": "rick@selby-family.co.uk"
    }
  ],
  "require": {
    "illuminate/contracts": "^10.0|^11.0",
    "php": "^8.1",
    "symfony/error-handler": "^6.0"
  },
  "require-dev": {
    "graham-campbell/testbench": "6.*",
    "phpunit/phpunit": "9.*",
    "symfony/http-kernel": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "RickSelby\\Laravel\\ExceptionHtml\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RickSelby\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": false
  }
}
